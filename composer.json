{
    "name": "rawaby88/muid",
    "version": "5.0.0",
    "description": "Laravel package for generating prefixed, validatable unique identifiers similar to Stripe's ID format",
    "keywords": ["laravel", "uuid", "identifier", "muid", "stripe-ids", "prefixed-ids"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rawaby88",
            "email": "info@rawaby88.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rawaby88\\Muid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawaby88\\Muid\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawaby88\\Muid\\MuidServiceProvider"
            ],
            "aliases": {
                "Muid": "Rawaby88\\Muid\\Facades\\Muid"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
